.box_div, .form, .footer{
  position: relative;
  margin: 10px 20px;
}

.box{
  width: 100%;
}

view.input{
  height: 40px;
  line-height: 30px;
}

.submit_btn{
  margin: 20px 0;
  border-radius: 35px;
  width: 33%;
}

.flex_box{
  display: flex;
  justify-content: left;
  /* width: 49.5%; */
  /* height: 120px; */
}

.flex_box navigator{
  width: 49%;
  margin: 0.5%;
  border:1px solid grey;
}

.left, .right{
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  max-width: 100%;
}

