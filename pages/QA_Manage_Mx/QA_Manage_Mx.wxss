.brand{
  display: block;
  /* border:1px solid red; */
  line-height: 0;
  float: left;
  width: 49%;
  /* height: 30px; */
}
.brand .input{
  margin: 0 !important;
  padding: 0 16px;
  line-height: 40px;
  /* border:1px solid rgb(255, 0, 242); */
}

.brand.type{
  margin-left: 2%;
}


.box{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: grey;
  z-index: 99;
}

.box .msg_box{
  position: absolute;
  width: 90%;
  height: 200px;
  padding: 5%;
  left: 5%;
  top: 30%;
  background: white;
  border-radius: 15px;
  z-index: 100;
}

.box .title9{
  text-align: center;
  font-weight: 600;
  color: rgb(32, 32, 32);
  margin: 6px 0 16px 0;
}
.box .msg{
  color: rgb(116, 116, 116);
  text-align: center;
  line-height: 30px;
}

.box .close_btn{
  margin: 0 !important;
  position: absolute;
  width: 90% !important;
  bottom: 5% !important;
  left: 5% !important;
}

.vin_tips{
  float: right;
  line-height: 22px;
  font-size: 12px;
  color: rgb(175, 124, 124);
}

.btn_div{
  margin-bottom: 30px;
}